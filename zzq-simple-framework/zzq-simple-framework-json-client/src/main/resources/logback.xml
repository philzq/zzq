<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <Pattern>
<!--                %d{yyyy-MM-dd HH:mm:ss} %-5level  [%-16.16thread]  %logger{35} %msg%n-->
                %d  %-5level  [%-16.16thread] %msg%n
            </Pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>